#include <bits/stdc++.h>
using namespace std;

class Bank {
public:
    vector<long long> bl;

    Bank(vector<long long>& balance) {
        bl = balance;
    }

    bool transfer(int account1, int account2, long long money) {
        if (account1 > bl.size() || account2 > bl.size() || account1 < 1 || account2 < 1)
            return false;
        if (bl[account1 - 1] >= money) {
            bl[account1 - 1] -= money;
            bl[account2 - 1] += money;
            return true;
        }
        return false;
    }

    bool deposit(int account, long long money) {
        if (account > bl.size() || account < 1) return false;
        bl[account - 1] += money;
        return true;
    }

    bool withdraw(int account, long long money) {
        if (account > bl.size() || account < 1) return false;
        if (bl[account - 1] >= money) {
            bl[account - 1] -= money;
            return true;
        }
        return false;
    }

    void printBalances() {
        cout << "Current balances: ";
        for (auto x : bl) cout << x << " ";
        cout << "\n";
    }
};

int main() {
    vector<long long> balances = {100, 200, 300, 400, 500};
    Bank bank(balances);

    cout << "Initial ";
    bank.printBalances();

    cout << "\nWithdraw 50 from account 1 -> " << (bank.withdraw(1, 50) ? "Success" : "Fail") << endl;
    bank.printBalances();

    cout << "\nDeposit 200 into account 3 -> " << (bank.deposit(3, 200) ? "Success" : "Fail") << endl;
    bank.printBalances();

    cout << "\nTransfer 100 from account 2 to account 5 -> " << (bank.transfer(2, 5, 100) ? "Success" : "Fail") << endl;
    bank.printBalances();

    cout << "\nTransfer 1000 from account 4 to account 2 -> " << (bank.transfer(4, 2, 1000) ? "Success" : "Fail") << endl;
    bank.printBalances();

    cout << "\nWithdraw 100 from invalid account 7 -> " << (bank.withdraw(7, 100) ? "Success" : "Fail") << endl;
    bank.printBalances();

    return 0;
}
